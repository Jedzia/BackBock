<wizard:StateItem
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="Jedzia.BackBock.Application.Editors.TaskWizard.TaskWizardFinalPage"
	x:Name="Window"
	xmlns:wizard="clr-namespace:Jedzia.BackBock.CustomControls.Wizard;assembly=Jedzia.BackBock.CustomControls"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	d:DesignWidth="640" d:DesignHeight="480" d:DataContext="{Binding TaskWizard, Mode=OneWay, Source={StaticResource Locator}}">


	<Grid x:Name="LayoutRoot">
		<StackPanel HorizontalAlignment="Left" Orientation="Vertical" VerticalAlignment="Top" Width="77">
			<Label x:Name="label" Content="Final" FontSize="29.333"/>
			<TextBox Text="{Binding Title}" />
			<TextBox Text="{Binding Path}" />
		</StackPanel>
	</Grid>
</wizard:StateItem>